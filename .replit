modules = ["nodejs-20", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["go", "libxcrypt", "libyaml", "pkg-config", "xsimd", "cargo", "hickory-dns", "inetutils", "sptk", "python313Packages.httpie", "gh", "solana-cli", "php", "pkg", "apk-tools", "nim-unwrapped", "vlang", "spidermonkey_128", "build2", "emacsclient-commands", "g-ls", "h", "python313Packages.open-interpreter", "ngn-k", "orbiton", "q-text-as-data", "timetrap", "yakut", "z-lua", "ruby_3_4", "python313Packages.llm", "zsh", "lua52Packages.lua", "webdav", "haskellPackages.wd", "nom", "pop", "mujoco", "got"]

[[ports]]
